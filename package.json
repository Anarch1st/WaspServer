{
  "name": "waspserver",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "bcrypt": "^3.0.3",
    "compression": "^1.7.3",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-proxy": "^1.17.0",
    "jsonfile": "^5.0.0",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "session-file-store": "^1.2.0",
    "uuid": "^3.3.2"
  },
  "repository": "git+https://github.com/Saii626/WaspServer.git",
  "author": "Saii",
  "license": "ISC",
  "homepage": "https://github.com/Saii626/WaspServer#readme",
  "description": "Nodejs server for Raspberry",
  "main": "server/waspserver.js",
  "devDependencies": {
    "nodemon": "^1.18.9"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "public/*",
      "node_modules/*",
      "server/data/db/*"
    ],
    "events": {
      "start": "notify-send WaspServer \"Server started\"",
      "crash": "notify-send WaspServer \"Server crashed\"",
      "exit": "notify-send WaspServer \"Server stopped\""
    }
  }
}